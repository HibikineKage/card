{
  "rulesDirectory": ["tslint-plugin-prettier"],
  "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
  "rules": {
    "import-name": false,
    "no-console": true,
    "prettier": [
      true,
      {
        "singleQuote": true,
        "semi": true,
        "trailingComma": "es5"
      }
    ]
  }
}
